{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "anonymous-messenger": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "front",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/front",
            "index": "front/index.html",
            "main": "front/main.ts",
            "polyfills": "front/polyfills.ts",
            "tsConfig": "front/tsconfig.front.json",
            "aot": true,
            "assets": [
              "front/favicon.ico",
              "front/assets"
            ],
            "styles": [
              "front/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "front/environments/environment.ts",
                  "with": "front/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "anonymous-messenger:build",
            "port": 4445
          },
          "configurations": {
            "production": {
              "browserTarget": "anonymous-messenger:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "anonymous-messenger:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "front/test.ts",
            "polyfills": "front/polyfills.ts",
            "tsConfig": "front/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "front/favicon.ico",
              "front/assets"
            ],
            "styles": [
              "front/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "front/tsconfig.front.json",
              "front/tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "anonymous-messenger:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "anonymous-messenger:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "anonymous-messenger",
  "cli": {
    "analytics": "fd50b6cb-02c1-4cc8-b1b9-d7d49fc24c53"
  }
}
