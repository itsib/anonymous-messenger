{
  "name": "anonymous-messenger",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently -n \"FRONT,SERVER\" \"npm run front\" \"npm run server\"",
    "front": "ng serve --disable-host-check",
    "server": "nodemon",
    "build": "npm run build:front && npm run build:server",
    "build:front": "ng build --prod",
    "build:server": "tsc --build server/tsconfig.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "hamburgers": "^1.1.3",
    "messageformat": "^2.3.0",
    "ngx-autosize": "^1.8.0",
    "rxjs": "~6.5.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "uuid": "^8.2.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/express": "^4.17.6",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "@types/uuid": "^8.0.0",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "http-proxy-middleware": "^1.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^2.0.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "exec": "ts-node --project server/tsconfig.json server/app.ts",
    "ext": "ts"
  }
}
